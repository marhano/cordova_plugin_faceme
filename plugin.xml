<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-faceme" version="1.0.0" 
xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>FaceMe</name>
    <js-module name="FaceMe" src="www/FaceMe.js">
        <clobbers target="FaceMe" />
    </js-module>
    <platform name="android">
        <resource-file src="src/android/libs/faceme-6.14.0.aar" target="libs/faceme-6.14.0.aar"/>
        <resource-file src="src/android/libs/faceme_widgets-6.14.0.aar" target="libs/faceme_widgets-6.14.0.aar"/>
        <resource-file src="src/android/activity_anti_spoofing.xml" target="res/layout/activity_anti_spoofing.xml" />
        <resource-file src="src/android/widget_strings.xml" target="res/values/widget_strings.xml" />
        <framework src="src/android/faceme.gradle" custom="true" type="gradleReference" />
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="FaceMe">
                <param name="android-package" value="inc.bastion.faceme.FaceMe" />
            </feature>
        </config-file>
        <config-file parent="/manifest/application" target="AndroidManifest.xml">
            <activity
                android:name="inc.bastion.faceme.AntiSpoofingActivity"
                android:theme="@style/Theme.AppCompat.NoActionBar"
                android:exported="false">
                <meta-data
                    android:name="android.app.lib_name"
                    android:value="" />
            </activity>
        </config-file>
        <source-file src="src/android/FaceMe.java" target-dir="src/inc/bastion/faceme" />
        <source-file src="src/android/FaceHolder.java" target-dir="src/inc/bastion/faceme" />
        <source-file src="src/android/FaceData.java" target-dir="src/inc/bastion/faceme" />
        <source-file src="src/android/RectUtil.java" target-dir="src/inc/bastion/faceme" />
        <source-file src="src/android/LicenseUtils.java" target-dir="src/inc/bastion/faceme" />
        <source-file src="src/android/AntiSpoofingActivity.java" target-dir="src/inc/bastion/faceme" />
    </platform>
</plugin>